mainmenu "Sample app Configuration"

if WIFI
config NET_SAMPLE_WIFI_SSID
	string "WIFI SSID - Network name"
	default "my_network"

config NET_SAMPLE_WIFI_PSK
	string "WIFI PSK - Network password key"
	default "secret_passwd"
endif

source "Kconfig.zephyr"
